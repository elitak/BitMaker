<?xml version="1.0"?>

<configuration>

    <configSections>
        <section
            name="bitMaker.miner"
            type="BitMaker.Miner.ConfigurationSection, BitMaker.Miner"/>
    </configSections>

    <bitMaker.miner>
        <pools>
            <add 
                url="http://wasabi.test:1234@api.bitcoin.cz:8332/" />
        </pools>
        <miners>
            <add
                type="BitMaker.Miner.Gpu.GpuMinerFactory, BitMaker.Miner.Gpu" />
        </miners>
    </bitMaker.miner>

    <startup>
        <supportedRuntime
            version="v4.0"
            sku=".NETFramework,Version=v4.5"/>
    </startup>

</configuration>
